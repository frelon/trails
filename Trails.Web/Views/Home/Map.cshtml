<div id="map"></div>
<script>
      function initMap() {
        var uluru = {lat: 58.0, lng: 18.0};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=@System.Configuration.ConfigurationManager.AppSettings["google-api-key"]&callback=initMap">
</script>